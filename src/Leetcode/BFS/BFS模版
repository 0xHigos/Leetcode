int BFS(Node root,Node target){
    Queue<Node> queue;
    int step=0;
    add root to queue;
    while(queue is not empty){
        step =step +1;
        int size =queue.size();
        for(int i=0;i<size;++i){
            Node cur =the first node in queue;
            return step if cur is target;

        }
    }
}